#: binsearch.c:20
msgid ""
"Binsearch using human\n"
"\n"
"Usage: ./binsearch [OPTIONS] \n"
"\n"
"    --version   print current version \n"
"    --help      print help \n"
"    -r          use roman numerals \n"
msgstr ""
"Бинарный поиск использующий человека\n"
"\n"
"Запуск: ./binsearch [опции] \n"
"\n"
"    --version   распечатать текущую версию \n"
"    --help      распечатать подсказку \n"
"    -r          использовать римские цифры \n"

#: binsearch.c:76
#, c-format
msgid "Choose number from [1..100] (in the mind)\n"
msgstr "Выберите число от [1..100] (в уме) \n"

#: binsearch.c:82
#, c-format
msgid "Is number > %s? (y/n or yes/no)\n"
msgstr "Число > %s? (д/н или да/нет)\n"

#: binsearch.c:84
msgid "y\n"
msgstr "д\n"

#: binsearch.c:85
msgid "yes\n"
msgstr "да\n"

#: binsearch.c:86
msgid "n\n"
msgstr "н\n"

#: binsearch.c:87
msgid "no\n"
msgstr "нет\n"

#: binsearch.c:90
#, c-format
msgid "The number: %s\n"
msgstr "Число: %s\n"
